
<%= form_for :wine, url: user_wines_path do |f| %>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :Notes %><br>
    <%= f.text_area :text %>
  </p>

    <p>
    <%= f.label :Vineyard %><br>
    <%= f.text_field :vineyard %>
  </p>

    <p>
    <%= f.label :Year %><br>
    <%= f.number_field :year %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>