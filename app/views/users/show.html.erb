 <div class="container">

  <h3>Wines <%= @user.name %> has tasted</h3>


<table cellspacing="0">
  <tr>

  </tr>
  <tr>
    <td>Type</td>
    <td>Vineyard</td>
    <td>Year</td>
    <td>Notes</td>
    <td>
  </tr>
    <% @user.wines.each do |wine| %>
  <tr>
    <td><%= wine.title %> </td>
    <td><%= wine.vineyard %></td>
    <td><%= wine.year %></td>  
    <td><%= wine.text %></td>
    <td><%= link_to 'View', user_wine_path(wine.user_id, wine.id) %> | <%= link_to 'Edit', edit_user_wine_path(wine.user_id, wine.id)%></td>
  </tr>
<% end %>

</table>


<p><p>
<%= link_to 'Back', users_path %> | 
<%= link_to 'Add Wine', new_user_wine_path(@user.id) %>


